{
  "name": "auth-server",
  "version": "1.0.0",
  "description": "JWT auth server",
  "author": "Daniel Lewis BSc(Hons)",
  "private": true,
  "scripts": {
    "start": "nodemon authServer.js",
    "forever": "pm2 start pm2.yaml",
    "test": "node conditionalTest",
    "enforcer": "./node_modules/.bin/istanbul check-coverage --statement 97 --branch 97 --function 97 --line 97",
    "build": "npm-run-all test enforcer",
    "postinstall": "test -d .git && cp gitHookPrePush.sh .git/hooks/pre-push || true"
  },
  "dependencies": {
    "app-root-path": "^2.0.1",
    "body-parser": "^1.17.0",
    "chalk": "^1.1.3",
    "cookie-parser": "^1.4.3",
    "express": "^4.15.0",
    "express-http-proxy": "^0.11.0",
    "express-validation": "^1.0.1",
    "globby": "^6.1.0",
    "joi": "^10.2.2",
    "jsonwebtoken": "^7.3.0",
    "moment": "^2.17.1",
    "request": "^2.79.0",
    "request-promise": "^4.1.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cookie": "^0.3.1",
    "data-driven": "^1.3.0",
    "eslint": "^3.16.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "nock": "^9.0.9",
    "npm-run-all": "^4.0.2",
    "supertest": "^3.0.0"
  },
  "engine": "node >= 6.10.0"
}
